{
    "name": "phpfastcgi/expressive-adapter",
    "description": "A PHPFastCGI adapter for the Zend Expressive framework",
    "keywords": ["expressive", "zend", "fastcgi", "server", "fast cgi", "daemon"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Carter",
            "email": "andrewcarter1992@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "zendframework/zend-expressive": "^1.0",
        "phpfastcgi/fastcgi-daemon": "^0.7"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PHPFastCGI\\Adapter\\Expressive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPFastCGI\\Test\\Adapter\\Expressive\\": "test/"
        }
    }
}
